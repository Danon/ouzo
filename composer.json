{
    "name": "letsdrink/ouzo",
    "description": "Ouzo PHP MVC framework",
    "minimum-stability": "dev",
    "license": "MIT",
    "keywords": ["php", "mvc", "framework", "orm", "tdd"],
    "authors": [
        {
            "name": "Thulium",
            "email": "letsdrink@thulium.pl"
        }
    ],
    "autoload": {
        "psr-0": {
            "Ouzo": "lib/"
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "sebastianbergmann/phpcov",
                "version": "1.1.0",
                "dist": {
                    "url": "https://github.com/sebastianbergmann/phpcov/archive/1.1.0.zip",
                    "type": "zip"
                },
                "source": {
                    "url": "https://github.com/sebastianbergmann/phpcov.git",
                    "type": "git",
                    "reference": "1.1.0"
                },
                "bin": [
                    "phpcov.php"
                ]
            }
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "3.7.19",
        "satooshi/php-coveralls": "dev-master",
        "sebastianbergmann/phpcov": "1.1.0"
    },
    "archive": {
        "exclude": ["/test", "/build", "/config", "/vendor", "/*.xml", "/*.sh", "/bootstrap_test.php"]
    }
}
